Before:
  let trex_right_footed = [
    \ "⠀⠀⠀⠀⠀⣴⣲⣶⣶⣦",
    \ "⠀⠀⠀⠀⠀⣿⣿⣟⣛⠛",
    \ "⣧⡀⢀⣤⣾⣿⣿⢤⠀⠀",
    \ "⠻⣿⣿⣿⣿⣿⡟⠀⠀⠀",
    \ "⠀⠈⢻⡿⠛⠯⠀⠀⠀⠀",
    \ "⠀⠀⠘⠂⠀⠀⠀⠀⠀⠀"
  \ ]
  let trex_left_footed = [
    \ "⠀⠀⠀⠀⠀⣴⣲⣶⣶⣦",
    \ "⠀⠀⠀⠀⠀⣿⣿⣟⣛⠛",
    \ "⣧⡀⢀⣤⣾⣿⣿⢤⠀⠀",
    \ "⠻⣿⣿⣿⣿⣿⡟⠀⠀⠀",
    \ "⠀⠈⠻⣟⠻⡏⠀⠀⠀⠀",
    \ "⠀⠀⠀⠀⠀⠓⠀⠀⠀⠀"
  \ ]

Execute (Update Trex to moment 1):
  call vim_trex_runner#trex#update_trex(1)

Then (GetTrexFrame returns list with trex standing on right leg):
  AssertEqual trex_right_footed, vim_trex_runner#trex#get_trex_frame()

Execute (Update Trex to moment 125):
  call vim_trex_runner#trex#update_trex(125)

Then (GetTrexFrame returns list with trex standing on left leg):
  AssertEqual trex_left_footed, vim_trex_runner#trex#get_trex_frame()

Execute (Update Trex to moment 250):
  call vim_trex_runner#trex#update_trex(250)

Then (GetTrexFrame returns list with trex standing on right leg):
  AssertEqual trex_right_footed, vim_trex_runner#trex#get_trex_frame()
